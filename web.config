<?xml version="1.0" encoding="utf-8"?>
<configuration>
<system.webServer>
    <handlers>
        <remove name="aspNetCore"/>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModule" resourceType="Unspecified"/>
    </handlers>
    <modules runAllManagedModulesForAllRequests="true" />
    <aspNetCore processPath="dotnet" arguments=".\Pronto.ValuationApi.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" />
</system.webServer>
</configuration>